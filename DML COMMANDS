| D05    |       40000 |    140000 |
| D06    |      150000 |    150000 |
+--------+-------------+-----------+
5 rows in set (0.00 sec)

mysql> select deptno,AVG(Salary) as AverageSalary
    -> from Employee
    -> where deptno <> 'D05'
    -> group by deptno;
+--------+---------------+
| deptno | AverageSalary |
+--------+---------------+
| D01    |    45000.0000 |
| D02    |    22000.0000 |
| D04    |    14500.0000 |
| D06    |   150000.0000 |
+--------+---------------+
4 rows in set (0.00 sec)

mysql> SELECT deptno, AVG(Salary) AS AverageSalary
    -> FROM Employee
    -> WHERE deptno <> 'D01'
    -> GROUP BY deptno
    -> HAVING AVG(Salary) > 20000
    -> ORDER BY AverageSalary;
+--------+---------------+
| deptno | AverageSalary |
+--------+---------------+
| D02    |    22000.0000 |
| D05    |    88750.0000 |
| D06    |   150000.0000 |
+--------+---------------+
3 rows in set (0.03 sec)

